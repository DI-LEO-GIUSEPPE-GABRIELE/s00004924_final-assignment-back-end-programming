type Query {
  flights(page: Int!, size: Int!): FlightPage!
  flight(id: ID!): Flight
}

type FlightPage {
  content: [Flight]
  totalPages: Int
  totalElements: Int
  number: Int
  size: Int
}

type Flight {
  id: ID!
  flightCode: String!
  departureDate: String!
  arrivalDate: String!
  priceCode: String
  status: String!
}
